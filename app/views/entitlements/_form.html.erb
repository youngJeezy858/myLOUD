<%= form_for @entitlement, :html => { :class => 'form-horizontal' } do |f| %>
<div class="control-group">
  <%= f.label :ami, :class => 'control-label' %>
  <div class="controls">
    <%= f.collection_select :ami, @amis, :imageId, :name %>
  </div>
</div>
<div class="form-actions">
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
  entitlements_path, :class => 'btn' %>
</div>
<% end %>
